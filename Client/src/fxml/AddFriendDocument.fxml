<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="524.0" prefWidth="853.0" stylesheets="@/css/AddFriend.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TableView fx:id="addTable" layoutX="22.0" layoutY="123.0" prefHeight="319.0" prefWidth="781.0">
            <columns>
                <TableColumn fx:id="userNameColumn" prefWidth="206.0" text="Username" />
                <TableColumn fx:id="fullNameColumn" prefWidth="363.0" text="Full Name" />
                <TableColumn fx:id="addColumn" minWidth="0.0" prefWidth="211.0" text="Send request" />
            </columns>
        </TableView>
        <Label layoutX="33.0" layoutY="24.0" prefHeight="41.0" prefWidth="136.0" text="Find friends">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <TextField fx:id="searchField" layoutX="169.0" layoutY="74.0" prefHeight="35.0" prefWidth="331.0" promptText="Enter Username or full name" />
        <Button fx:id="searchBtn" onAction="#handleSearchButton" layoutX="516.0" layoutY="74.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="121.0" text="Search">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="backBtn" layoutX="680.0" layoutY="455.0" mnemonicParsing="false" onAction="#handleBackAction" prefHeight="20.0" prefWidth="121.0" text="BACK" />
    </children>
</AnchorPane>
